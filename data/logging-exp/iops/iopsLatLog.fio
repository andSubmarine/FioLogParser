[global]
# parameters and levels
bs=4k
iodepth=1
numjobs=1
# config
direct=1
thread=1
filename=/dev/nvme0n1
ioengine=external:/usr/lib/libxnvme-fio-engine.so
disable_clat=1
disable_slat=1
group_reporting

[ss-write]
size=512g
time_based
rw=randwrite
runtime=2h
ss=iops:10%
ss_dur=7m
ss_ramp=10s
stonewall

[test]
rw=randwrite
size=200G
write_iops_log=test2
write_lat_log=test2
stonewall

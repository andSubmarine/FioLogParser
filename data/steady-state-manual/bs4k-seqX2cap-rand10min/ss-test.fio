[global]
# parameters and levels
bs=4K
iodepth=1
numjobs=1
# config
size=512G
direct=1
thread=1
group_reporting
filename=/dev/nvme0n1
ioengine=external:/usr/lib/libxnvme-fio-engine.so
disable_clat=1
disable_slat=1
disable_bw=1

[ss-seq]
rw=write
size=1024G
write_lat_log=ss-seq
stonewall

[ss-rand]
rw=randwrite
runtime=10m
time_based
write_lat_log=ss-rand
stonewall

[test]
rw=write
runtime=5m
time_based
write_lat_log=test
stonewall